data = input("Digite a data do seu nascimento separada por '/': ")
dia, mes, ano = data.split("/")
mes_extenso = {
    "01": "janeiro",
    "02": "fevereiro",
    "03": "mar√ßo",
    "04": "abril",
    "05": "maio",
    "06": "junho",
    "07": "julho",
    "08": "agosto",
    "09": "setembro",
    "10": "outubro",
    "11": "novembro",
    "12": "dezembro"}
mes_1 = mes_extenso[mes]
print("Voce nasceu no dia", dia, "de", mes_1, "de", ano)
